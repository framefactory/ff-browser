{
    "name": "@framefactory/browser",
    "version": "2.1.0",
    "license": "MIT",
    "description": "FF Browser Typescript Foundation Library",
    "keywords": [
        "typescript",
        "browser",
        "client"
    ],
    "homepage": "https://github.com/framefactory/ff-browser#readme",
    "author": {
        "name": "Ralph Wiedemeier",
        "email": "opensource@framefactory.ch",
        "url": "https://github.com/framefactory"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/framefactory/ff-browser.git"
    },
    "bugs": {
        "url": "https://github.com/framefactory/ff-browser/issues"
    },
    "files": [
        "build/types/**/*",
        "build/esm/**/*",
        "build/cjs/**/*",
        "src/**/*"
    ],
    "exports": {
        "./*": {
            "types": "./build/types/*.d.ts",
            "import": "./build/esm/*.js",
            "require": "./build/cjs/*.js"
        }
    },
    "imports": {
        "#lib/*": "./build/cjs/*.js"
    },
    "_moduleAliases": {
        "@ff/core": "../core/cjs/*.js"
    },
    "scripts": {
        "build": "npm run clean && npm run build:esm && npm run build:cjs",
        "build:esm": "tsc -b tsconfig.esm.json",
        "build:cjs": "tsc -b tsconfig.json",
        "clean": "rm -rf ./build",
        "test": "rm -rf ./build/test && tsc -b test && mocha -c --ui tdd build/test/test.js"
    },
    "devDependencies": {
        "@types/chai": "^4.3.3",
        "@types/mocha": "^9.1.1",
        "chai": "^4.3.6",
        "mocha": "^10.0.0",
        "module-alias": "^2.2.2",
        "typescript": "^4.8.3"
    }
}
